Device MyPi
    type: RPi4
    sensors:
        - HCSR04
        - VL53L0X
    actuators:
        - GenericLedArray
    networking: CSAL
    communication: HomeEMQX
    rmm:
        communication: Redis
        control:
            volume: true
            ssh: true
        monitoring:
            health: false
            logs: false
        applications:
            types:
                - ROS
                - R4A
end
